<!--
    route: /blog/atom.atom
-->
<?xml version='1.0' encoding='UTF-8'?>
<feed xmlns='http://www.w3.org/2005/Atom'>
  <id>tag:manveru.dev,2011:blog</id>
  <updated>{{.updated}}</updated>
  <title type='text'>manveru</title>
  <link rel='alternate' type='text/html' href='https://manveru.dev/blog'/>
  <link rel='self' type='application/atom+xml' href='https://manveru.dev/blog/atom.atom'/>
  <author>
    <name>Michael 'manveru' Fellinger</name>
  </author>
  <generator version="2019.08" uri="http://github.com/manveru/sagenix">SageNix</generator>
  {{range .posts}}
    <entry>
      <id>tag:manveru.dev,2011:blog,{{.date}}</id>
      <published>{{.date}}</published>
      <updated>{{.date}}</updated>
      <author><name>Michael 'manveru' Fellinger</name></author>
      <title type="text">{{.title}}</title>
      <content type="html">{{string .body}}</content>
    </entry>
  {{end}}
</feed>
